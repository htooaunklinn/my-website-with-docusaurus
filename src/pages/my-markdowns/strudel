// "Midnight Bloom" â€“ chill lofi with piano + drums + power elements
samples('github:eddyflux/crate')
setcps(.120)
â€‹
// CHORDS: Looped mellow jazz piano chords
let chords = chord("<Am7 Dm7 G7 Cmaj7>/2").dict('ireal')
â€‹
// Full stack
stack(
  // ğŸ¹ PIANO CHORDS (looped)
  chords.voicing()
    .s("gm_epiano1")
    .gain(0.9)
    .room(0.4)
    .phaser(3)
    .slow(2),
â€‹
  // ğŸ¹ PIANO MELODY (arpeggiated notes)
  n("[0 1 2 3 4 3 2 1]*2")
    .set(chords)
    .mode("arp:up")
    .s("gm_epiano1")
    .gain(0.6)
    .delay(.125)
    .clip(0.8)
    .room(0.3),
â€‹
  // ğŸ¥ DRUM BEAT â€“ chill + bounce
  stack(
    s("bd").struct("x ~ x ~"),
    s("hh").struct("~ x ~ x"),
    s("sd").struct("~ ~ x ~")
  ).bank("crate")
   .gain(1)
   .mask("<1 1 1 0>/16"),
â€‹
  // ğŸ”Š PUNCHY BASS
  n("[0 ~ 0 ~ 5 5 3 3]")
    .set(chords)
    .mode("root:c2")
    .s("gm_slap_bass_1")
    .gain(0.7)
    .lpf(300),
  // ğŸŒŒ AMBIENT TEXTURE FX
  s("vinyl:loop").gain(0.1).slow(4),
  s("wind").gain(0.05).delay(0.2)
)
.size(4)
.late("[0 .01]*2")
